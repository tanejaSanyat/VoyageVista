ng && (\n                    <div className=\"flex items-center justify-center mx-auto\">\n                      <div className=\"w-40 h-40 border-t-4 border-b-4 border-green-900 rounded-full animate-spin\"></div>\n                    </div>\n                  )}\n\n                  {!!booking && booking.length > 0 ? (\n                    booking.map((booking) => (\n                      <tr key={booking._id}>\n                        <td className=\"px-6 py-4\">\n                          <div className=\"flex items-center space-x-3\">\n                            <div>\n                              <p> {booking?.title} </p>\n                            </div>\n                          </div>\n                        </td>\n                        <td className=\"px-6 py-4\">\n                          <p className=\"\">{booking?.tour_date}</p>\n                        </td>\n                        <td className=\"px-6 py-4\">\n                          <p>${booking?.package_price}</p>\n                        </td>\n\n                        <td className=\"px-6 py-4 text-center\">\n                          {booking?.create_at}\n                        </td>\n                        <td className=\"px-6 py-4 text-center\">\n                          <span\n                            className={`text-white text-sm w-1/3 pb-1 b capitalize font-semibold px-2 rounded-full ${\n                              booking.status === \"pending\"\n                                ? \"bg-red-400\"\n                                : \"bg-green-600\"\n                            }`}\n                          >\n                            <button\n                              className=\"btn btn-primary mt-2\"\n                              onClick={() =>\n                                statusButton(booking?._id, booking?.status)\n                              }\n                            >\n                              {booking?.status}\n                            </button>\n                          </span>\n                        </td>\n                        <td className=\"px-6 py-4\">\n                          <p>{booking?.name}</p>\n                        </td>\n\n                        <td className=\"px-6 py-4 text-center\">\n                          <button\n                            onClick={() => handleDelete(booking?._id)}\n                            className=\"text-red-500 hover:underline\"\n                          >\n                            <i className=\"far fa-trash-alt text-2xl \"></i>\n                          </button>\n                        </td>\n                      </tr>\n                    ))\n                  ) : (\n                    <h1 className=\"text-center\">Data Not Found.</h1>\n                  )}\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </main>\n      </div>\n    </div>\n  );\n};\n\nexport default ManageBooking;\n","/Users/sanyattaneja/Downloads/VoyageVista-main/src/pages/Dashboard/MyBooking.jsx",[],"/Users/sanyattaneja/Downloads/VoyageVista-main/src/pages/Dashboard/Dashboard.jsx",[],"/Users/sanyattaneja/Downloads/VoyageVista-main/src/pages/Register/Register.jsx",["146"],"import React, { useState } from \"react\";\nimport { useForm } from \"react-hook-form\";\nimport { Link, useHistory, useLocation } from \"react-router-dom\";\nimport Breadcrumbs from \"../../components/Breadcrumbs/Breadcrumbs\";\nimport { useAuth } from \"../../components/Context/AuthContext\";\nimport SocialLogin from \"../../components/SocialLogin/SocialLogin\";\n\nconst Register = () => {\n  const [error, setError] = useState(\"\");\n  const [loading, setLoading] = useState(false);\n  const { signUp } = useAuth();\n\n  const history = useHistory();\n  const location = useLocation();\n  const redirect_url = location.state?.from || \"/\";\n\n  const {\n    register,\n    handleSubmit,\n    formState: { errors },\n  } = useForm();\n\n  const onSubmit = async (data) => {\n    if (data.email !== data.password) {\n      setError(\"Passwords do not match\");\n    }\n\n    try {\n      setError(\"\");\n      setLoading(true);\n      await signUp(data.email, data.password);\n      history.push(redirect_url);\n    } catch (error) {\n      setError(error.message);\n    }\n    setLoading(false);\n  };\n\n  return (\n    <>\n      <Breadcrumbs title=\"Register\" />\n      <div className=\"container\">\n        <div\n          className=\"flex gap-8 items-center justify-center \"\n          style={{ minHeight: \"75vh\" }}\n        >\n          <div\n            className=\"inline-block border-2 border-gray-200 p-8 rounded-md shadow-lg\"\n            style={{ width: \"450px\" }}\n          >\n            <form onSubmit={handleSubmit(onSubmit)}>\n              <h2 className=\"text-2xl font-Poppins text-center\">\n                Log In Your Account\n              </h2>\n              {error && (\n                <span className=\"text-red-600 text-center py-2\">{error}</span>\n              )}\n\n              <div className=\"mt-4 mb-4\">\n                <label htmlFor=\"email\">Email</label>\n                <input\n                  className=\"border border-gray-400 w-full px-4 py-2\"\n                  placeholder=\"Enter Email\"\n                  {...register(\"email\", { required: true })}\n                />\n                {errors.email && (\n                  <span className=\"text-red-600\">This field is required</span>\n                )}\n              </div>\n\n              <div className=\"mt-4 mb-4\">\n                <label htmlFor=\"password\">Password</label>\n                <input\n                  type=\"password\"\n                  placeholder=\"Password\"\n                  className=\"border border-gray-400 w-full px-4 py-2\"\n                  {...register(\"password\", { required: true })}\n                />\n                {errors.password && (\n                  <span className=\"text-r